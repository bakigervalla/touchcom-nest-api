{
  "name": "touchcom-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "crud:create": "nest g -c nestjs-prisma-crud-schematics crud-resource",
    "start": "nest start --exec \"node --max-http-header-size=20000\"",
    "start:dev": "nest start --watch --exec \"node --max-http-header-size=20000\"",
    "start:debug": "nest start --debug --watch --exec \"node --max-http-header-size=20000\"",
    "start:prod": "npx prisma migrate deploy && npx prisma db seed && node --max-http-header-size=20000 dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch --exec \"node --max-http-header-size=20000\"",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "dotenv -e .env.test -- npx prisma migrate reset --force --skip-seed && dotenv -e .env.test -- jest --runInBand --config ./test/jest-e2e.json ;",
    "local:db:create": "docker run --name touchcom-postgres -e POSTGRES_PASSWORD=localhost -d -p 5432:5432 postgres",
    "prisma:init": "npx prisma init",
    "prisma:introspect": "npx prisma introspect",
    "prisma:client": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:seed": "npx prisma db seed",
    "prettier:check": "prettier --check 'src/**/*.{js,jsx,ts,tsx}' 'test/**/*.{js,jsx,ts,tsx}'",
    "husky:prepare": "husky install"
  },
  "dependencies": {
    "@google-cloud/logging-winston": "^6.0.0",
    "@google-cloud/storage": "^7.1.0",
    "@nestjs/common": "^10.2.6",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.6",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/microservices": "^10.2.6",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.2.6",
    "@nestjs/swagger": "^7.1.12",
    "@nestjs/throttler": "^6.2.1",
    "@prisma/client": "^5.3.1",
    "@sendgrid/mail": "^7.7.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "firebase-admin": "^11.11.0",
    "lodash": "^4.17.21",
    "mqtt": "^5.0.5",
    "nest-winston": "^1.9.4",
    "nestjs-prisma-crud": "1.0.0-beta.18",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.0",
    "twilio": "^4.18.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.18",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.6",
    "@types/express": "^4.17.18",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "29.5.5",
    "@types/multer": "^1.4.8",
    "@types/node": "^20.8.2",
    "@types/passport-jwt": "^3.0.10",
    "@types/supertest": "^2.0.14",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "jest": "29.7.0",
    "jest-mock-extended": "^3.0.5",
    "nestjs-prisma-crud-schematics": "^1.0.0-beta.12",
    "prettier": "^3.0.3",
    "prisma": "^5.3.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "moduleDirectories": [
      "<rootDir>/../",
      "node_modules"
    ],
    "moduleNameMapper": {
      "@common/(.*)": "<rootDir>/src/common/$1",
      "@twilio/(.*)": "<rootDir>/src/twilio/$1",
      "@auth/(.*)": "<rootDir>/src/auth/$1",
      "@generics/(.*)": "<rootDir>/src/generics/$1",
      "@users/(.*)": "<rootDir>/src/users/$1",
      "@sites/(.*)": "<rootDir>/src/sites/$1",
      "@roles/(.*)": "<rootDir>/src/roles/$1",
      "@permissions/(.*)": "<rootDir>/src/permissions/$1",
      "@configurations/(.*)": "<rootDir>/src/configurations/$1",
      "@version/(.*)": "<rootDir>/src/version/$1",
      "@mqtt/(.*)": "<rootDir>/src/mqtt/$1",
      "@devices/(.*)": "<rootDir>/src/devices/$1",
      "@access-control/(.*)": "<rootDir>/src/access-control/$1",
      "@access-times/(.*)": "<rootDir>/src/access-times/$1",
      "@access-groups/(.*)": "<rootDir>/src/access-groups/$1",
      "@access-key/(.*)": "<rootDir>/src/access-key/$1",
      "@access-key-provider/(.*)": "<rootDir>/src/access-key-provider/$1",
      "@mocks(.*)": "<rootDir>/src/mocks",
      "@~google-cloud/(.*)": "<rootDir>/src/google-cloud/$1",
      "@~prisma/(.*)": "<rootDir>/src/prisma/$1",
      "@src/(.*)": "<rootDir>/src/$1"
    },
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 60,
        "lines": 60,
        "statements": 60
      }
    },
    "testEnvironment": "node"
  },
  "prisma": {
    "seed": "ts-node prisma/seeders/seed.ts"
  }
}
